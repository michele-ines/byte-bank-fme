{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "noEmit": false
  },
    "include": [
    "src",
    "src/**/*.d.ts",
    "../types/global-assets.d.ts",
    "../components/my-cards/**/*",
    "../components/my-cards/**/*"
  ]
}
